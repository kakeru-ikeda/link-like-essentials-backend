# Server Configuration
NODE_ENV=development
PORT=4000

# Database (Neon PostgreSQL)
LLES_DATABASE_URL=postgresql://user:password@host:5432/database?schema=public

# Upstash Redis (REST API)
# ローカル開発はserverless-redis-httpを使用
UPSTASH_REDIS_REST_URL=http://localhost:8079
UPSTASH_REDIS_REST_TOKEN=example_token

# Sukisuki Club Redis (REST API)
SUKISUKI_CLUB_HTTP_REDIS_URL=http://localhost:8080
SUKISUKI_CLUB_HTTP_REDIS_TOKEN=example_token

# Firebase Authentication
LLES_FIREBASE_SERVICE_ACCOUNT_PATH=firebase-service-account.json
LLES_FIREBASE_PROJECT_ID=link-like-essentials

# CORS
LLES_CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=info

# Sentry
SENTRY_DSN=
